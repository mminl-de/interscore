# Necessary for clangd to work properly with Qt6
CompileFlags:
  Add: [
	"-DQT_CORE_LIB",
	"-DQT_GUI_LIB",
	"-DQT_MULTIMEDIA_LIB",
	"-DQT_NETWORK_LIB",
	"-DQT_WIDGETS_LIB",
	"-I/usr/include/qt6",
	"-I/usr/include/qt6/QtCore",
	"-I/usr/include/qt6/QtGui",
	"-I/usr/include/qt6/QtMultimedia",
	"-I/usr/include/qt6/QtNetwork", # TODO CONSIDER
	"-I/usr/include/qt6/QtWidgets",
	"-I/usr/lib/qt6/mkspecs/linux-g++",
	"-lQt6Core",
	"-lQt6Gui",
	"-lQt6Multimedia",
	"-lQt6Widgets"
]
